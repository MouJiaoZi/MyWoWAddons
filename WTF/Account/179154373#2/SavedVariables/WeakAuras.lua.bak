["customText"] = "function ()\n    if aura_env.CurrentMarker.Name then \n        return \"|c00FF7D0A[M+ AUTO MARK] Current marker: |r\"..aura_env.CurrentMarker.Name;\n    else \n        return \"|c00FF7D0A[M+ AUTO MARK] Current marker: |r|c00FFFFFFNone|r\";\n    end\nend",
